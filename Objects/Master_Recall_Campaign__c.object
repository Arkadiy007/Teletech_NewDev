        <fullName>Campaign_Type__c</fullName>
        <description>Translates Campaign Type Code from 3 to Service and 4 to Safety</description>
        <externalId>false</externalId>
        <formula>IF(TEXT(Recall_Type_Code__c) = &quot;3&quot;, 
&quot;Service&quot;, 
(IF(TEXT(Recall_Type_Code__c) = &quot;4&quot;, 
&quot;Safety&quot;, 
&quot;N/A&quot;))
)</formula>
