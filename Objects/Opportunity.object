    <fields>
        <fullName>Incentive_Level_Commercial__c</fullName>
        <description>NCV/Fleet: Evaluates Total Order Quantity to determine Special Incentive volume level</description>
        <externalId>false</externalId>
        <formula>IF( RecordTypeId = &quot;012Z00000008V62&quot;,
(IF(TOQty__c &gt;= 3 &amp;&amp; TOQty__c &lt;= 9, &quot;Fleet 1&quot;,
(IF(TOQty__c &gt;= 10 &amp;&amp; TOQty__c &lt;= 25, &quot;Fleet 2&quot;,
(IF(TOQty__c &gt;= 26 &amp;&amp; TOQty__c &lt;= 249, &quot;Fleet 3&quot;,
(IF(TOQty__c &gt;= 250, &quot;Fleet 4&quot;, &quot;No Discount&quot;)))))))),
&quot;Not Fleet&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Incentive Level Commercial</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Incentive_Level_Rental__c</fullName>
        <description>NCV/Fleet: Evaluates Total Order Quantity to determine Special Incentive volume level for Rentals</description>
        <externalId>false</externalId>
        <formula>IF( RecordTypeId = &quot;012Z00000008V63&quot;,
(IF(TOQty__c &gt;= 0 &amp;&amp; TOQty__c &lt;= 14999, &quot;Rental 1&quot;,
(IF(TOQty__c &gt;= 15000, &quot;Rental 2&quot;, &quot;No Discount&quot;)))),
&quot;Not Rental&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Incentive Level Rental</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
